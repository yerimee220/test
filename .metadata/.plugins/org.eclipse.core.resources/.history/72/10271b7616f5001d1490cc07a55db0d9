		int [] numArr = {1, 10, 3, 2, 1, 2, 2, 5, 6, 3, 11};
		int [] resultArr = new int[10]; 
		// resultArr [0]은 1의 출현횟수저장공간 [1]은 2의 저장공간
		int b=0;
		for(int i=0; i<11; i++)	
		if(numArr[i]==1) {
			resultArr[0]=b++;
