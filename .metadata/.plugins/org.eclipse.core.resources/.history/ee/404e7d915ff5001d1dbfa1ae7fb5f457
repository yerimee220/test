
public class TV {
	 boolean power; 
	 int ch;
	 int vol;
	public int sum(int a, int b) {
		return a+b;
	}
	public void powerOn() {
		power = true; 
	}
	
	public boolean isPower() {	
		return power;
	}
	
	public void print () {
		System.out.println("채널은"+ch+","+"음량은"+vol);
	}
	
	public void powerOff() {
		power = false; 
	}
	
	}


	

