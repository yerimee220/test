
public class TV {
	public boolean power; 
	public int ch;
	public int vol;
	
	public void powerOn() {
		power = true; 
	}
	
	public boolean isPower() {	
		return power;
	}
	
	public void print () {
		System.out.println("채널은"+ch+","+"음량은"+vol);
	}
	
	public void powerOff() {
		power = false; 
	}
	
	}


	

