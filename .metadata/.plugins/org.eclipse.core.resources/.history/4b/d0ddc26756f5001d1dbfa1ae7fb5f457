
public class TV {
//재료 - 전원(), 채널, 음량
	boolean power;
	int channel = 0; 
    
	
	/**
	 * TV를 켠다
	 */
	public void powerOn() {
		power = true;
	}
	
	/**
	 * TV를 끈다 
	 */
	public void powerOff() {
		power = false;
	}
	
	public void isPower() {
		if(power==true) {
		}	
	}
	
	public void print() {
		if(power=true) {
		System.out.println("채널은"+channel+"번");	
		}
		}
	}


	

